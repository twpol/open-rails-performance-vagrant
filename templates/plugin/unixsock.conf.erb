<Plugin unixsock>
  SocketFile  "<%= @socketfile %>"
  SocketGroup "<%= @socketgroup %>"
  SocketPerms "<%= @socketperms %>"
<% if scope['::collectd::collectd_version'] and (scope.function_versioncmp([scope['::collectd::collectd_version'], '5']) >= 0) -%>
  DeleteSocket "<%= @deletesocket %>"
<% end -%>
</Plugin>
