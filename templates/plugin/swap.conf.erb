<Plugin swap>
  ReportByDevice <%= @reportbydevice %>
<% if scope['::collectd::collectd_version'] and scope.function_versioncmp([scope['::collectd::collectd_version'], '5.2']) > 0 -%>
  ReportBytes <%= @reportbytes %>
<% end -%>
<% if scope['::collectd::collectd_version'] and scope.function_versioncmp([scope['::collectd::collectd_version'], '5.5']) > 0 -%>
  ValuesAbsolute = <%= @valuesabsolute %>
  ValuesPercentage = <%= @valuespercentage %>
<% end -%>
</Plugin>
