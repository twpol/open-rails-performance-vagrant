<Plugin logfile>
  LogLevel <%= @log_level %>
  File "<%= @log_file %>"
  Timestamp <%= @log_timestamp %>
<% if scope['::collectd::collectd_version'] and (scope.function_versioncmp([scope['::collectd::collectd_version'], '4.10']) >= 0) -%>
  PrintSeverity <%= @print_severity %>
<% end -%>
</Plugin>
