<Plugin disk>
<% @disks.each do |disk| -%>
  Disk  "<%=disk %>"
<% end -%>
  IgnoreSelected <%= @ignoreselected %>
<% if @udevnameattr and scope['::collectd::collectd_version'] and (scope.function_versioncmp([scope['::collectd::collectd_version'], '5.5']) >= 0) -%>
  UdevNameAttr <%= @udevnameattr %>
<% end -%>
</Plugin>
